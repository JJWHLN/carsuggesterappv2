    "@types/react-window": "^1.8.8",
    "detox": "^20.40.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "react-devtools-core": "^6.1.5",
    "react-test-renderer": "18.3.1",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.0",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "private": true
