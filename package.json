  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "^0.22.3",
    "@expo/webpack-config": "^19.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/detox": "^17.14.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
