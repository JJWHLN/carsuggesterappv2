{
  "name": "car-marketplace-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "lint": "expo lint",
    "build": "expo build",
    "build:analyze": "expo build --analyze",
    "bundle:analyze": "npx expo-bundle-analyzer --platform android",
    "bundle:size": "node scripts/bundle-size-check.js",
    "optimize": "node scripts/production-optimizer.js",
    "optimize:analyze": "node scripts/production-optimizer.js && npm run bundle:analyze",
    "preload:routes": "node scripts/preload-routes.js",
    "performance:monitor": "node scripts/performance-monitor.js",
    "clean:cache": "expo r -c && npm run clean:metro",
    "clean:metro": "npx expo r --clear",
    "build:production": "npm run optimize && expo build --release-channel production",
    "analyze:bundle": "npm run bundle:analyze",
    "check:performance": "npm run performance:monitor",
    "preload:critical": "node scripts/preload-critical-resources.js",
    "optimize:images": "node scripts/optimize-images.js",
    "cache:clear": "node scripts/clear-caches.js"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
